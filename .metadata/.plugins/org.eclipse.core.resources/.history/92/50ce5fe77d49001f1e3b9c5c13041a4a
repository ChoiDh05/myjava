package arrayTest;

public class MyArrayTest {
public static void main(String[] args) {
	int[] arData = new int[5];   // 저장공간
//	arData[0] = 120;      // 저장공간
//	arData[0] + 9;      // 값
	System.out.println(arData);   // 주소값
	arData[2] = arData[0] + arData[1];   // 저장공간, 값, 값
	System.out.println(arData[5]);      // 오류
}
}
