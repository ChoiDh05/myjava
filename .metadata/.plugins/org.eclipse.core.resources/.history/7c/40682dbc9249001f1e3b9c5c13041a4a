package myJAVA;

public class PC {
	private String graphicsCard;
	private String cpu;
	private String mainbord;
	private String ram;
	private String power;
	private String pcCase;
	private String cooler;
	
	public PC() {;}
	
	public PC (PC pc) {
		this(pc.getGraphicsCard(), pc.getCpu(), pc.getMainbord(), pc.getRam(), pc.getPower(), pc.getPcCase(),pc.getCooler());
	}

	public PC(String graphicsCard, String cpu, String mainbord, String ram, String power, String pcCase,
			String cooler) {
		super();
		this.graphicsCard = graphicsCard;
		this.cpu = cpu;
		this.mainbord = mainbord;
		this.ram = ram;
		this.power = power;
		this.pcCase = pcCase;
		this.cooler = cooler;
	}

	public String getGraphicsCard() {
		return graphicsCard;
	}

	public void setGraphicsCard(String graphicsCard) {
		this.graphicsCard = graphicsCard;
	}

	public String getCpu() {
		return cpu;
	}

	public void setCpu(String cpu) {
		this.cpu = cpu;
	}

	public String getMainbord() {
		return mainbord;
	}

	public void setMainbord(String mainbord) {
		this.mainbord = mainbord;
	}

	public String getRam() {
		return ram;
	}

	public void setRam(String ram) {
		this.ram = ram;
	}

	public String getPower() {
		return power;
	}

	public void setPower(String power) {
		this.power = power;
	}

	public String getPcCase() {
		return pcCase;
	}

	public void setPcCase(String pcCase) {
		this.pcCase = pcCase;
	}

	public String getCooler() {
		return cooler;
	}

	public void setCooler(String cooler) {
		this.cooler = cooler;
	}

	@Override
	public String toString() {
		return "PC [graphicsCard=" + graphicsCard + ", cpu=" + cpu + ", mainbord=" + mainbord + ", ram=" + ram
				+ ", power=" + power + ", pcCase=" + pcCase + ", cooler=" + cooler + "]";
	}
	
	
}
